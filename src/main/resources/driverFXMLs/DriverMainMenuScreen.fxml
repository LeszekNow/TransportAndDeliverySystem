<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.leszeknowinski.controllers.driverControllers.DriverMainMenuController">
   <children>
      <VBox alignment="CENTER" layoutX="75.0" layoutY="85.0" prefHeight="176.0" prefWidth="159.0" spacing="5.0">
         <children>
            <Button fx:id="startOrder" mnemonicParsing="false" onAction="#loadStartOrderScreen" prefWidth="125.0" text="Start order" />
            <Button fx:id="showOrders" mnemonicParsing="false" onAction="#loadAllOrdersScreen" prefWidth="125.0" text="Show orders" />
            <Button fx:id="finishOrder" mnemonicParsing="false" onAction="#loadFinishOrderScreen" prefWidth="125.0" text="Finish order" />
            <Button fx:id="showVehicle" onAction="#loadVehiclesShowScreen" layoutX="27.0" layoutY="116.0" mnemonicParsing="false" prefWidth="125.0" text="Show vehicle" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
         <opaqueInsets>
            <Insets left="50.0" top="50.0" />
         </opaqueInsets>
      </VBox>
      <Label layoutX="95.0" layoutY="44.0" text="Driver Main Menu">
         <font>
            <Font name="System Italic" size="14.0" />
         </font>
      </Label>
      <VBox alignment="CENTER" layoutX="75.0" layoutY="268.0" prefHeight="78.0" prefWidth="159.0" spacing="5.0">
         <children>
            <Button fx:id="back" mnemonicParsing="false" onAction="#getBack" prefWidth="125.0" text="Back" />
            <Button fx:id="exit" mnemonicParsing="false" onAction="#getOut" prefWidth="125.0" text="Exit" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
         <opaqueInsets>
            <Insets left="50.0" top="50.0" />
         </opaqueInsets>
      </VBox>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</Pane>
