<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="450.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.leszeknowinski.controllers.adminManagerControllers.EmployeeMainMenuController">
   <children>
      <VBox alignment="CENTER" layoutX="45.0" layoutY="85.0" prefHeight="176.0" prefWidth="159.0" spacing="5.0">
         <children>
            <Button fx:id="addNewCar" mnemonicParsing="false" onAction="#chooseVehicleType" prefWidth="125.0" text="Add new car" />
            <Button fx:id="addNewEmployee" mnemonicParsing="false" onAction="#chooseEmployeeType" prefWidth="125.0" text="Add new employee" />
            <Button fx:id="addNewCustomer" mnemonicParsing="false" onAction="#chooseCustomerType" prefWidth="125.0" text="Add new customer" />
            <Button fx:id="addNewLocation" mnemonicParsing="false" onAction="#addLocation" prefWidth="125.0" text="Add new location" />
            <Button fx:id="addNewRoute" mnemonicParsing="false" onAction="#addRoute" prefWidth="125.0" text="Add new route" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
         <opaqueInsets>
            <Insets left="50.0" top="50.0" />
         </opaqueInsets>
      </VBox>
      <VBox alignment="CENTER" layoutX="225.0" layoutY="85.0" prefHeight="176.0" prefWidth="159.0" spacing="5.0">
         <children>
            <Button fx:id="deleteVehicle" mnemonicParsing="false" onAction="#deleteVehicle" prefWidth="125.0" text="Delete vehicle" />
            <Button fx:id="deleteEmployee" mnemonicParsing="false" onAction="#deleteEmployee" prefWidth="125.0" text="Delete employee" />
            <Button fx:id="deleteCustomer" mnemonicParsing="false" onAction="#deleteCustomer" prefWidth="125.0" text="Delete customer" />
            <Button fx:id="deleteLocation" mnemonicParsing="false" onAction="#deleteLocation" prefWidth="125.0" text="Delete location" />
            <Button fx:id="deleteRoute" mnemonicParsing="false" onAction="#deleteRoute" prefWidth="125.0" text="Delete route" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </VBox>
      <VBox alignment="CENTER" layoutX="45.0" layoutY="235.0" prefHeight="176.0" prefWidth="159.0" spacing="5.0">
         <children>
            <Button fx:id="showVehicles" mnemonicParsing="false" onAction="#showVehicleTypeChooseScreen" prefWidth="125.0" text="Show vehicles" />
            <Button fx:id="showCustomers" mnemonicParsing="false" onAction="#showCustomers" prefWidth="125.0" text="Show customers" />
            <Button fx:id="trackVehicle" mnemonicParsing="false" onAction="#loadVehicleShowScreen" prefWidth="125.0" text="Track vehicle" />
            <Button fx:id="calculations" mnemonicParsing="false" prefWidth="125.0" text="Calculations" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
         <opaqueInsets>
            <Insets left="50.0" top="50.0" />
         </opaqueInsets>
      </VBox>
      <Label layoutX="151.0" layoutY="51.0" text="Employee Main Menu">
         <font>
            <Font name="System Italic" size="14.0" />
         </font>
      </Label>
      <VBox alignment="CENTER" layoutX="225.0" layoutY="314.0" prefHeight="78.0" prefWidth="159.0" spacing="5.0">
         <children>
            <Button fx:id="back" mnemonicParsing="false" onAction="#getBack" prefWidth="125.0" text="Back">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Button>
            <Button fx:id="exit" mnemonicParsing="false" onAction="#getOut" prefWidth="125.0" text="Exit">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Button>
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
         <opaqueInsets>
            <Insets left="50.0" top="50.0" />
         </opaqueInsets>
      </VBox>
      <VBox alignment="CENTER" layoutX="225.0" layoutY="249.0" prefHeight="89.0" prefWidth="159.0" spacing="5.0">
         <children>
            <Button fx:id="customerMenu" mnemonicParsing="false" onAction="#loadCustomerMenu" prefWidth="125.0" text="Customer Menu" />
            <Button fx:id="driverMenu" mnemonicParsing="false" onAction="#loadDriverMenu" prefWidth="125.0" text="Driver Menu" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </VBox>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</Pane>
