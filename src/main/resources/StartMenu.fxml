<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="450" prefWidth="850" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.leszeknowinski.controllers.StartMenuController">
   <children>
      <Label alignment="CENTER" layoutX="127.0" layoutY="125.0" prefHeight="82.0" prefWidth="611.0" text="Welcome in com.leszeknowinski TRANSPORTATION SERVICES" textAlignment="CENTER" wrapText="true">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font></Label>
      <ChoiceBox fx:id="userType" layoutX="303.0" layoutY="259.0" onAction="#loadOptions" prefWidth="150.0" />
      <Label layoutX="304.0" layoutY="242.0" text="Select an item:" />
      <Button fx:id="go" layoutX="463.0" layoutY="259.0" mnemonicParsing="false" onAction="#displayUserMenu" prefWidth="85.0" text="Go" />
      <Button fx:id="exit" layoutX="1254.0" layoutY="717.0" mnemonicParsing="false" onAction="#getOut" prefWidth="85.0" text="Exit" />
      <Label fx:id="message" layoutX="533.0" layoutY="263.0" prefHeight="32.0" prefWidth="546.0" textAlignment="CENTER" textFill="#f40606">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
   </children>
</Pane>
