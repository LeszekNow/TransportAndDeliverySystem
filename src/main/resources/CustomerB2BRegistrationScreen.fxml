<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="350.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.leszeknowinski.controllers.CustomerB2BRegistrationController">
   <children>
      <VBox alignment="TOP_RIGHT" layoutX="8.0" layoutY="23.0" prefHeight="279.0" prefWidth="161.0" spacing="13.0">
         <children>
            <Label text="Company name:" />
            <Label text="NIP number:" />
            <Label text="phone number:" />
            <Label text="Username:" />
            <Label text="Password:" />
         </children>
      </VBox>
      <VBox alignment="TOP_CENTER" layoutX="172.0" layoutY="19.0" prefHeight="288.0" prefWidth="161.0" spacing="5.0">
         <children>
            <TextField fx:id="company" />
            <TextField fx:id="nip" />
            <TextField fx:id="phone" />
            <TextField fx:id="username" />
            <TextField fx:id="password" />
         </children>
      </VBox>
      <HBox layoutX="147.0" layoutY="306.0" prefHeight="35.0" prefWidth="195.0" spacing="10.0">
         <children>
            <Button fx:id="back" mnemonicParsing="false" onAction="#getBack" prefWidth="85.0" text="Back" />
            <Button fx:id="exit" mnemonicParsing="false" onAction="#getOut" prefWidth="85.0" text="Exit" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </HBox>
      <Button fx:id="submit" layoutX="247.0" layoutY="277.0" mnemonicParsing="false" onAction="#createCustomerB2B" prefWidth="85.0" text="Sumbit" />
      <Label fx:id="message" layoutX="23.0" layoutY="182.0" prefHeight="35.0" prefWidth="307.0" textAlignment="CENTER" textFill="#6eff60" wrapText="true">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
   </children>
</Pane>
